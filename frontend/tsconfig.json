        "sourceMap": true,
        "noImplicitAny": true,
        "module": "commonjs",
        "target": "es6",
        "jsx": "react",
        "allowJs": true,
        "esModuleInterop": true,
        "baseUrl": "./",
        "paths": {
            "*": [
                "node_modules/*"
            ],
